    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^5.0.2",
    "bumpp": "^10.1.0",
    "happy-dom": "^17.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsdown": "^0.11.9",
